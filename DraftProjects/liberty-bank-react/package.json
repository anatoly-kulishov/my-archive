{
  "name": "liberty",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "tsc": "tsc --noEmit",
    "build": "tsc && vite build",
    "prepare": "husky install",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 ",
    "format": "prettier --no-error-on-unmatched-pattern --write src/**/*.{js,jsx,ts,tsx,scss,md,json} --config ./.prettierrc",
    "preview": "vite preview",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "typecheck": "tsc --noEmit && eslint ."
  },
  "dependencies": {
    "@2gis/mapgl": "^1.40.0",
    "@2gis/mapgl-clusterer": "^2.5.0",
    "@hookform/resolvers": "^3.3.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/react-test-renderer": "^18.0.7",
    "dayjs": "^1.11.10",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "js-sha256": "^0.10.1",
    "react": "^18.2.0",
    "react-calendar": "^4.6.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-date-picker": "^10.5.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.46.1",
    "react-imask": "^7.1.3",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.16.0",
    "react-test-renderer": "^18.2.0",
    "whatwg-fetch": "^3.6.19",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/types": "^17.4.4",
    "@jest/environment": "^29.7.0",
    "@storybook/addon-essentials": "7.4.5",
    "@storybook/addon-interactions": "7.4.5",
    "@storybook/addon-links": "7.4.5",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/blocks": "7.4.5",
    "@storybook/react": "7.4.5",
    "@storybook/react-vite": "7.4.5",
    "@storybook/testing-library": "0.2.1",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.8.7",
    "@types/prettier": "^3",
    "@types/react": "^18.2.15",
    "@types/react-copy-to-clipboard": "^5.0.6",
    "@types/react-datepicker": "^4.19.5",
    "@types/react-dom": "^18.2.7",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.14",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "imask": "^7.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.2",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.65.1",
    "sharp": "^0.32.6",
    "storybook": "7.4.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.9",
    "vite-plugin-image-optimizer": "^1.1.7",
    "vite-plugin-svgr": "^4.2.0"
  },
  "engines": {
    "npm": "please-use-yarn",
    "node": ">= 18.17.0",
    "yarn": ">= 1.22.19"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config-git-commit/.commitizen.cjs"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md,json}": [
      "yarn format",
      "git add -A ."
    ]
  }
}
